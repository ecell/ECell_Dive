{
  "metadata": [
    {
      "src": [
        {
          "files": ["Assembly-CSharp.csproj"],
          "cwd": ".",
          "exclude": ["**/obj/**", "**/bin/**", "docs/**"]
        }
      ],
      "dest": "docs/ScriptingAPI/meta"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "meta/**.yml",
          "api/*.md",
          "articles/**.md",
          "toc.yml"
        ],
        "cwd": "docs/ScriptingAPI"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "docs/_site/ScriptingAPI",
    "template": [
      "default"
    ]
  }
}