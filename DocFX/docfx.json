{
  "metadata": [
    {
      "src": [
        {
            "src": "../",
            "files": [
                "Assets/**/*.cs"
            ],
            "exclude": [
                "Assets/Plugins/**"
            ]
        }
    ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "allowCompilationErrors": true,
      "includePrivateMembers": true,
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "FusionApiDev/toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "favicon.ico",
          "logo.svg",
          "resources/images/**/**.png",
          "resources/images/**/**.jpg",
          "resources/images/**/**.gif"
        ]
      }
    ],
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}